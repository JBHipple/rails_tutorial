<h1>All Tags</h1>

<ul id="tags">
	<% @tags.each do |tag| %>
		<li>	
			<%= link_to tag.name, tag_path(tag) %> - <% if logged_in? %><%= link_to "Delete tag", tag_path(tag), method: :delete, data: {confirm: "Really delete the tag?"} %><% end %>
		</li>
	<% end %>
</ul>
<br><br><br>
<%= link_to "<< Back to Articles List", articles_path %>